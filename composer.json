{
    "name": "dasunr/omnipay-nabtransact",
    "type": "library",
    "description": "NAB Transact driver for the Omnipay payment processing library",
    "keywords": [
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment",
        "nabtransact"
    ],
    "homepage": "https://github.com/dasunr/nabtransact",
    "license": "MIT",
    "authors": [
        {
            "name": "Dasun R",
            "email": "dasunr@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": { 
            "Omnipay\\NABTransact\\" : "src/"
        }
    },
    "require": {
        "cweagans/composer-patches": "~1.6",
        "omnipay/common": "*"
    },
    "require-dev": {
        "omnipay/tests": "~2.0",
        "satooshi/php-coveralls": "1.0.0",
        "fzaninotto/faker": "~1.4"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        },
        "patches": {
            "guzzle/guzzle": {
                "Update CA": "patches/guzzle-guzzle-update-cacert.patch"
            }
        }
    }
}
